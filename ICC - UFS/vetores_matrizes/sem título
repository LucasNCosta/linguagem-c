#include<stdio.h>
#include<string.h>

int main(){
	
	char p1[50];
	int quantidade,i,tamanho;
	
	quantidade =0;
	
	
	printf("Digite uma palavra\n");
	fflush(stdin);	
	gets(p1);
	
	tamanho = strlen(p1);
	
	for(i=0;i<=tamanho;i++){
		
		if(p1[i]=='e'){
			quantidade=quantidade+1;
		}
	}
		
	printf("\nA quantidade de A na palavra eh %d", quantidade);
	
	
	return 0;
}
